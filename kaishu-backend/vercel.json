{
  "version": 2,
  "name": "kaishu-backend",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    },
    "server.js": {
      "maxDuration": 30
    }
  },
  "regions": [
    "hkg1"
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kaishu.z-l.top"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kaishu-frontend-git-main-xiaolongmrs-projects.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kaishu-backend.z-l.top"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kaishu-backend.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kaishu-backend.z-l.top"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kaishu-backend.vercel.app"
        },
        {
          "key": "Vercel-CORS-Mode",
          "value": "dynamic"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kaishu.z-l.top"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kaishu-frontend-git-main-xiaolongmrs-projects.vercel.app"
        },
        {
          "key": "Vercel-CORS-Mode",
          "value": "dynamic"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}