# 数据库配置
DATABASE_URL=postgresql://username:password@localhost:5432/kaishu

# JWT密钥
JWT_SECRET=your-super-secret-jwt-key-here

# 百度OCR配置
BAIDU_OCR_API_KEY=your-baidu-ocr-api-key
BAIDU_OCR_SECRET_KEY=your-baidu-ocr-secret-key

# Cloudinary云存储配置（可选）
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Supabase云存储配置（可选）
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-supabase-service-role-key
# 或者使用匿名密钥（权限较低）
# SUPABASE_KEY=your-supabase-anon-key

# 前端URL配置（用于CORS）
FRONTEND_URL=http://localhost:5173

# 服务器配置
PORT=3005
NODE_ENV=development

# Vercel部署配置
VERCEL_URL=your-app-name.vercel.app

# 生产环境前端URL（Vercel部署后）
PRODUCTION_FRONTEND_URL=https://your-frontend-app.vercel.app